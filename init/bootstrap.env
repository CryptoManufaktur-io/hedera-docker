# PostgreSQL environment variables
export PGUSER="postgres"
export PGPASSWORD="postgres_password"
export PGDATABASE="postgres"
export PGHOST="db"
export PGPORT="5432"

# Is the DB a GCP Cloud SQL instance?
export IS_GCP_CLOUD_SQL="false"

export CREATE_MIRROR_API_USER="true"

# Set DB users' passwords
export GRAPHQL_PASSWORD="postgres_password"
export GRPC_PASSWORD="postgres_password"
export IMPORTER_PASSWORD="postgres_password"
export OWNER_PASSWORD="postgres_password"
export REST_PASSWORD="postgres_password"
export REST_JAVA_PASSWORD="postgres_password"
export ROSETTA_PASSWORD="postgres_password"
export WEB3_PASSWORD="postgres_password"